@model IEnumerable<WebSignalrDemo.Models.User>
@{ 
    Layout = null;
}

<h4>Kullanıcılar</h4>
<div class="row">
    <div class="col-lg-6">
        @foreach (var item in Model)
        {
            <ul class="list-unstyled">
                <li>
                    <a>@item.Username</a>
                </li>
            </ul>

        }

    </div>
</div>
